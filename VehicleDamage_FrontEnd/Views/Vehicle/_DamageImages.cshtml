@model IEnumerable<byte[]>

@if (Model.Count() == 0)
{
    <div class="w3-panel w3-yellow w3-center">
        <h3>Oops!There seems to be no images.</h3>
    </div>
}
else
{
    foreach (byte[] image in Model)
    {
        <img src="data:image/jpeg;base64,@Convert.ToBase64String(image)" width="291" height="285" />
    }
}